lexer.c: $(srcdir)/lexer.l
	@rm -f $@
	$(LEX) $(LFLAGS) -t $< | sed -e 's/yy/__IDL_/g' >$@

parser.c parser.h: $(srcdir)/parser.y
	$(YACC) $(YFLAGS) $<
	test -f y.tab.c && mv -f y.tab.c parser.c
	test -f y.tab.h && mv -f y.tab.h parser.h

